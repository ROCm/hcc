/*
 * type casts
 */
#define CONVERT_A_B(INTN,ISUFF,FLOATN,FSUFF) \
	FUNC(convert_##ISUFF##_##FSUFF,INTN,convert_##INTN,(FLOATN x),1) \
	FUNC(convert_##FSUFF##_##ISUFF,FLOATN,convert_##FLOATN,(INTN x),1)

//unsigned to signed	
#define CONVERT_UA_B(SRC,USRC,USRCSUFF,DEST,DESTSUFF) \
	COMPLEX(convert_##DESTSUFF##_##USRCSUFF,DEST,convert_ ##DEST(as_ ##USRC(%)) ,(USRC x)) \
	COMPLEX(convert_##USRCSUFF##_##DESTSUFF,SRC,as_ ##SRC(convert_ ##USRC(%)),(DEST x))
	
	//unsigned to signed	
#define CONVERT_UA_UB(SRC,USRC,USRCSUFF,DEST,UDEST,UDESTSUFF) \
	COMPLEX(convert_##USRCSUFF##_##UDESTSUFF,SRC,as_ ##SRC(convert_ ##USRC(as_ ##UDEST(%))) ,(UDEST x)) \
	COMPLEX(convert_##UDESTSUFF##_##USRCSUFF,DEST,as_ ##DEST(convert_ ##UDEST(as_ ##USRC(%))),(USRC x))
	



